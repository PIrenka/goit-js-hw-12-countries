{{#each this}}
{{!-- <section> --}}

    <h2>{{name}}</h2>

    <h3>Capital:</h3>
    <p>{{capital}}</p>

    <h3>Population:</h3>
    <p>{{population}}</p>

    <h3>Languages:</h3>
    {{!-- <ul class="js-languages-list"> --}}
        <ul>
            {{#each languages}}
            <li>
                <p>{{this.name}}</p>
            </li>
            {{/each}}
        </ul>

        {{!--
</section> --}}
{{/each}}